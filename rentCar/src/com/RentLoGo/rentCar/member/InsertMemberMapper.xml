<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
      PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
   "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
   
<mapper namespace="com.RentLoGo.rentCar.member.InsertMemberMapper">

	<resultMap id="insertMemberResult" type="MemberDTO">
		<result property="memberId" column="member_id" />
		<result property="memberPw" column="member_pw" />
		<result property="memberClass" column="member_class" />
		<result property="memberName" column="member_name" />
		<result property="memberBirth" column="member_birth" />
		<result property="memberPhone" column="member_phone" />
		<result property="memberEmail" column="member_email" />
		<result property="memberDate" column="member_date" />
	</resultMap>
	
	<insert id="insertMember" parameterType="MemberDTO"> 
	<!-- resultMap="insertMemberResult" > -->
<!-- 	<select id="insertMember" resultType="com.RentLoGo.rentCar.model.CarDTO"> -->
      <![CDATA[
		insert into "member"("member_id", "member_pw", "member_class", "member_name", 
							"member_birth", "member_phone", "member_email")
					values(#{memberId}, #{memberPw}, 1, #{memberName}, 
							#{memberBirth}, #{memberPhone}, #{memberEmail}) 
      ]]>
	</insert>
	
</mapper>